<Window x:Class="DiagramDesigner.OpenUrlForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DiagramDesigner"
        mc:Ignorable="d"
        Title="OpenUrlForm" Height="460" Width="750">
    <Grid>
        <Label  Content="URL :" FontSize="24" HorizontalAlignment="Left" Margin="66.5,126.7,0,0" VerticalAlignment="Top"/>
        <TextBox Name="url" HorizontalAlignment="Left" Height="37" Margin="148,132,0,0" TextWrapping="Wrap" Text="" FontSize="24" VerticalAlignment="Top" Width="566"/>
        <Button Content="OK" FontSize="24" HorizontalAlignment="Left" Margin="618,348,0,0" VerticalAlignment="Top" Width="96" Click="Save" Height="47"/>
        <Button Content="Cancel" FontSize="24" HorizontalAlignment="Left" Margin="467,348,0,0" VerticalAlignment="Top" Width="105" Height="47"/>
        <Grid Height="88" VerticalAlignment="Top" Background="Red">
            <TextBlock Text="Open URL Form" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="50" FontFamily="Calibri"/>
        </Grid>
    </Grid>
</Window>
