<Window x:Class="DiagramDesigner.OpenUrlForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DiagramDesigner"
        mc:Ignorable="d"
        Title="OpenUrlForm" Height="460" Width="620">
    <Grid>
        <Grid Height="88" VerticalAlignment="Top" Background="SkyBlue">
            <TextBlock Text="Open URL Form" Foreground="White" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="50" FontFamily="Calibri"/>
        </Grid>
        <Label  Content="URL :" FontSize="24" HorizontalAlignment="Left" Margin="50,116.6,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.766,-1"/>
        <TextBox Name="url" HorizontalAlignment="Left" Height="37.1" Margin="50,179.6,0,0" TextWrapping="Wrap" Text="" FontSize="24" VerticalAlignment="Top" Width="522"/>
        <Button Content="OK" FontSize="24" HorizontalAlignment="Left" Margin="476,346,0,0" VerticalAlignment="Top" Width="96" Click="Save" Height="38"/>
        <Button Content="Cancel" FontSize="24" HorizontalAlignment="Left" Margin="326,348,0,0" VerticalAlignment="Top" Width="105"/>

    </Grid>
</Window>
