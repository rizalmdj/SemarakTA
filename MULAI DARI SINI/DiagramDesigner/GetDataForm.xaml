        <TextBox Name ="nama" HorizontalAlignment="Left" Height="37.1" Margin="98.5,157.2,0,0" TextWrapping="Wrap" Text="" FontSize="24" VerticalAlignment="Top" Width="442.5"/>
        <TextBox Name ="xpath" HorizontalAlignment="Left" Height="37.1" Margin="98.5,260,0,0" TextWrapping="Wrap" Text="" FontSize="24" VerticalAlignment="Top" Width="442.5"/>
        <Button Content="OK" FontSize="24" HorizontalAlignment="Left" Margin="443,348,0,0" VerticalAlignment="Top" Width="96"  Click="Save"/>
